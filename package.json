{
  "name": "typescript-book-library",
  "version": "1.0.0",
  "description": "Interactive TypeScript CLI library demonstrating recursion, classes, async functions, exceptions, TSLint and Jest.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "lint": "tslint -c tslint.json -p tsconfig.json -t stylish 'src/**/*.ts'",
    "test": "jest --coverage",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.0"
  }
}
